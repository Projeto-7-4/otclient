Module
  name: client_entergame
  description: Manages enter game and character list windows
  author: edubart & otclient.ovh
  website: https://github.com/edubart/otclient
  scripts: [ entergame, characterlist, castslist ]
  @onLoad: EnterGame.init() CharacterList.init() CastsList.init()
  @onUnload: EnterGame.terminate() CharacterList.terminate() CastsList.terminate()
  
  load-later:
    - game_things
    - game_features
